                alert(err.message);\n            });\n        },\n        registerWithGoogle: function() {\n            const provider = new firebase.auth.GoogleAuthProvider();\n            firebase.auth().signInWithPopup(provider)\n            .then(result => {\n                alert(`Account created for ${result.user.email}`);\n                this.$router.go({path: this.$router.path});\n            })\n            .catch(err => {\n                alert(err.message);\n            });\n        }\n    }\n};\n</script>\n\n<style>\ninput {\n    border: 1px solid black !important;\n}\n</style>\n"],"mappings":"AA6BA,OAAO,KAAKA,QAAO,MAAO,cAAc;AACxC;AACA,OAAO,eAAe;AAEtB,SAASC,aAAY,QAAS,cAAc;AAC5C,SAASC,OAAO,EAAEC,kBAAkB,EAAEC,8BAA8B,EAAEC,eAAc,QAAS,eAAe;AAE5G,MAAMC,WAAU,GAAIL,aAAa,CAACM,cAAc,CAAC;AACjD,MAAMC,IAAG,GAAIN,OAAO,CAACI,WAAW,CAAC;AAEjC,eAAe;EACXG,IAAI,EAAE,cAAc;EACpBC,IAAI,EAAE,SAAAA,CAAA,EAAW;IACb,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,QAAQ,EAAE,SAAAA,CAAA,EAAW;MACjBd,QAAQ,CAACQ,IAAI,CAAC,CAAC,CAACJ,8BAA8B,CAAC,IAAI,CAACO,KAAK,EAAE,IAAI,CAACC,QAAQ,EACvEG,IAAI,CAACC,IAAG,IAAK;QACVC,KAAK,CAAE,uBAAsBD,IAAI,CAACA,IAAI,CAACL,KAAM,EAAC,CAAC;QAC/C,IAAI,CAACO,OAAO,CAACC,EAAE,CAAC;UAACC,IAAI,EAAE,IAAI,CAACF,OAAO,CAACE;QAAI,CAAC,CAAC;MAC9C,CAAC,EACAC,KAAK,CAACC,GAAE,IAAK;QACVL,KAAK,CAACK,GAAG,CAACC,OAAO,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDC,kBAAkB,EAAE,SAAAA,CAAA,EAAW;MAC3B,MAAMC,QAAO,GAAI,IAAIzB,QAAQ,CAACQ,IAAI,CAACL,kBAAkB,CAAC,CAAC;MACvDH,QAAQ,CAACQ,IAAI,CAAC,CAAC,CAACH,eAAe,CAACoB,QAAQ,EACvCV,IAAI,CAACW,MAAK,IAAK;QACZT,KAAK,CAAE,uBAAsBS,MAAM,CAACV,IAAI,CAACL,KAAM,EAAC,CAAC;QACjD,IAAI,CAACO,OAAO,CAACC,EAAE,CAAC;UAACC,IAAI,EAAE,IAAI,CAACF,OAAO,CAACE;QAAI,CAAC,CAAC;MAC9C,CAAC,EACAC,KAAK,CAACC,GAAE,IAAK;QACVL,KAAK,CAACK,GAAG,CAACC,OAAO,CAAC;MACtB,CAAC,CAAC;IACN;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}