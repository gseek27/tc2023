      firebase.auth().createUserWithEmailAndPassword(this.email, this.password)\n            .then(user => {\n                alert(`Account created for ${user.user.email}`);\n                this.$router.go({path: this.$router.path});\n            })\n            .catch(err => {\n                alert(err.message);\n            });\n        },\n        registerWithGoogle: function() {\n            const provider = new firebase.auth.GoogleAuthProvider();\n            firebase.auth().signInWithPopup(provider)\n            .then(result => {\n                alert(`Account created for ${result.user.email}`);\n                this.$router.go({path: this.$router.path});\n            })\n            .catch(err => {\n                alert(err.message);\n            });\n        }\n    }\n};\n</script>\n\n<style>\ninput {\n    border: 1px solid black !important;\n}\n</style>\n"],"mappings":"AA6BA,OAAOA,QAAO,MAAO,cAAc;AACnC,OAAO,eAAe;AAGtB,MAAMC,cAAa,GAAI;EACnBC,MAAM,EAAE,yCAAyC;EACjDC,UAAU,EAAE,8BAA8B;EAC1CC,SAAS,EAAE,cAAc;EACzBC,aAAa,EAAE,0BAA0B;EACzCC,iBAAiB,EAAE,cAAc;EACjCC,KAAK,EAAE,2CAA2C;EAClDC,aAAa,EAAE;AACjB,CAAC;AAEHR,QAAQ,CAACS,aAAa,CAACR,cAAc,CAAC;AACtC;;AAEA,eAAe;EACXS,IAAI,EAAE,cAAc;EACpBC,IAAI,EAAE,SAAAA,CAAA,EAAW;IACb,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,QAAQ,EAAE,SAAAA,CAAA,EAAW;MACjBf,QAAQ,CAACgB,IAAI,CAAC,CAAC,CAACC,8BAA8B,CAAC,IAAI,CAACL,KAAK,EAAE,IAAI,CAACC,QAAQ,EACvEK,IAAI,CAACC,IAAG,IAAK;QACVC,KAAK,CAAE,uBAAsBD,IAAI,CAACA,IAAI,CAACP,KAAM,EAAC,CAAC;QAC/C,IAAI,CAACS,OAAO,CAACC,EAAE,CAAC;UAACC,IAAI,EAAE,IAAI,CAACF,OAAO,CAACE;QAAI,CAAC,CAAC;MAC9C,CAAC,EACAC,KAAK,CAACC,GAAE,IAAK;QACVL,KAAK,CAACK,GAAG,CAACC,OAAO,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDC,kBAAkB,EAAE,SAAAA,CAAA,EAAW;MAC3B,MAAMC,QAAO,GAAI,IAAI5B,QAAQ,CAACgB,IAAI,CAACa,kBAAkB,CAAC,CAAC;MACvD7B,QAAQ,CAACgB,IAAI,CAAC,CAAC,CAACc,eAAe,CAACF,QAAQ,EACvCV,IAAI,CAACa,MAAK,IAAK;QACZX,KAAK,CAAE,uBAAsBW,MAAM,CAACZ,IAAI,CAACP,KAAM,EAAC,CAAC;QACjD,IAAI,CAACS,OAAO,CAA